{
  "del": [
    "*SpriteAtlasTexture*.webp"
  ],
  "del_dir": [
    "avg/common",
    "story/info/obt/guide",
    "story/obt/guide",
    "story/obt/legion",
    "story/info/obt/roguelike",
    "stroy/obt/roguelike",
    "stroy/obt/rogue",
    "stroy/obt/rune",
    "stroy/obt/sandboxperm",
    "stroy/obt/tutorial"
  ],
  "opt": [
    {
      "file": "*.wav",
      "action": "ffmpeg_audio",
      "options": {
        "format": "mp3",
        "bitrate": 320
      }
    },
    {
      "file": "*.webp",
      "action": "pillow_webp",
      "options": {
        "format": "webp",
        "quality": 90
      }
    },
    {
      "file": "gamedata/*.txt",
      "action": "zstd",
      "options": {
        "level": 11,
        "min": 512
      }
    },
    {
      "file": "gamedata/*.json",
      "action": "zstd",
      "options": {
        "level": 11,
        "min": 512
      }
    }
  ],
  "compress_dir": [
    "avg/characters/",
    "avg/effects/background/",
    "avg/effects/battle/",
    "avg/effects/image/"
  ]
}